    <% } %>
    <% if(success && success.length > 0){ %>
    <div class="alert alert-success" role="alert">
      <%= success %>
    </div>
    <% } %>
  </div>

  <div id="landing-header">
    <!-- <h4>I am just a girl<br /> who obsessed with indoor plants</h4> -->
    <a href="/plants" class="btn btn-lg btn-success">Welcome to Sun's corner</a>

